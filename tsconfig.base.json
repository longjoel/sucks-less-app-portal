{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@slap/sdk": ["packages/sdk/src/index.ts"],
      "@slap/ui": ["packages/ui/src/index.tsx"],
      "@slap/calculator": ["apps/calculator/src/index.tsx"],
      "@slap/journal": ["apps/journal/src/index.tsx"],
      "@slap/mh-phq9": ["apps/mh-phq9/src/index.tsx"],
      "@slap/mh-gad7": ["apps/mh-gad7/src/index.tsx"],
      "@slap/mh-abc": ["apps/mh-abc/src/index.tsx"],
      "@slap/box-breathing": ["apps/box-breathing/src/index.tsx"],
      "@slap/daily-checklist": ["apps/daily-checklist/src/index.tsx"],
      "@slap/game-2048": ["apps/game-2048/src/index.tsx"],
      "@slap/minesweeper": ["apps/minesweeper/src/index.tsx"],
      "@slap/compass": ["apps/compass/src/index.tsx"],
      "@slap/countdown": ["apps/countdown/src/index.tsx"],
      "@slap/dice-roller": ["apps/dice-roller/src/index.tsx"],
      "@slap/minute-timer": ["apps/minute-timer/src/index.tsx"],
      "@slap/simon-says": ["apps/simon-says/src/index.tsx"],
      "@slap/ski-free": ["apps/ski-free/src/index.tsx"],
      "@slap/stopwatch": ["apps/stopwatch/src/index.tsx"],
      "@slap/mastermind": ["apps/mastermind/src/index.tsx"],
      "@slap/sudoku": ["apps/sudoku/src/index.tsx"],
      "@slap/fireplace": ["apps/fireplace/src/index.tsx"],
      "@slap/aquarium": ["apps/aquarium/src/index.tsx"]
    },
    "target": "ES2020",
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "strict": true,
    "noEmit": true,
    "jsx": "react-jsx"
  }
}
